{
  "SeriLog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Warning",
        "System": "Warning"
      },
      "WriteTo": [
        { "Name": "Console" }
      ]
    }
  }
}
